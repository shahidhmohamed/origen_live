  <template id="origen_theme.service_contact_template" name="Service contact Template">
    <section class="services_contact">
      <div class="wrapper">
        <div>
          <h2>Contact us</h2>
          <div class="contact_feature">
            <div><img class="svg" src="/origen_theme/static/src/img/icons/checkbox.svg" alt="Checked checkbox"/></div>
            <div>
              <h4>We are a technical team</h4>
              <p>Feel free to include all your requirements or questions in details, we know our stuff</p>
            </div>
          </div>
          <div class="contact_feature">
            <div><img class="svg" src="/origen_theme/static/src/img/icons/conf.svg" alt="Video conference"/></div>
            <div>
              <h4>Prefer another medium?</h4>
              <p>Include in your message whatever other means of communication you prefer, be it phone, IM, or video conference</p>
            </div>
          </div>
          <div class="contact_feature">
            <div><img class="svg icon_phone" src="/origen_theme/static/src/img/icons/phone.svg" alt="Phone"/></div>
            <div>
              <h4>Want to chat? Call us!</h4>
              <p><a href="tel:+18556967443" class="button yellow">Dial +1 855 696 7443</a></p>
            </div>
          </div>
        </div>

        <div class="form contact_form">
          <form action="/website_form/" method="post" data-model_name="crm.lead" data-success_page="/contactus-thank-you" enctype="multipart/form-data" data-editable-form="false" class="s_website_form">
            <div class="input_group">
              <label for="">Full name:</label>
              <input type="text" required="true" name="name" t-att-value="request.params.get('name', '')"/>
            </div>
            <div class="input_group">
              <label for="">Email address:</label>
              <input type="email" required="true" name="email_from" t-att-value="request.params.get('email_from', '')" />
            </div>
            <div class="input_group">
              <label for="">Company name:</label>
              <input type="text" name="partner_name" t-att-value="request.params.get('partner_name', '')"/>
            </div>

            <div class="input_group">
              <label for="">Your message:</label>
              <textarea name="description" id="" cols="30" rows="10" required="true"><t t-esc="request.params.get('description', '')"/></textarea>
            </div>
            <input type="hidden" name="source_page" t-att-value="request.httprequest.url"/>
            <div class="input_group send_button">
              <span class="button blue send">Send</span>
              <i class="fa fa-spinner fa-pulse fa-2x fa-fw d-none"></i>
              <span class="btn btn-primary btn-lg o_website_form_send d-none">Send</span>
              <span id="o_website_form_result"></span>
            </div>
            <div class="error_messages d-none">
              <span class="section_title">Name</span>
              <span data-field="name" data-rule="required">Please fill in your name</span>
              <span class="section_title">Email</span>
              <span data-field="email_from" data-rule="required">Please provide your email</span>
              <span data-field="email_from" data-rule="email">Please enter a valid email</span>
              <span class="section_title">Company</span>
              <span data-field="partner_name" data-rule="required"></span>
              <span class="section_title">Message</span>
              <span data-field="description" data-rule="required">Please write your message</span>
            </div>
          </form>
        </div>
      </div>
    </section>
  </template>



