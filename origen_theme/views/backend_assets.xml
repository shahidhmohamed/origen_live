    <record id="origen_theme.backend_layout" model="ir.ui.view">
        <field name="name">Backend Layout</field>
        <field name="inherit_id" ref="web.layout"/>
        <field name="priority">100</field>
        <field name="arch" type="xml">
            <xpath expr="//title" position="replace">
                <title t-esc="title or 'Origen'"/>
            </xpath>

            <xpath expr="//link[@type='image/x-icon']" position="replace">
                <link type="image/x-icon" rel="shortcut icon" href="/origen_theme/static/src/img/favicon.ico"/>
            </xpath>

        </field>
    </record>
</odoo>